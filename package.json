{
  "name": "coc-utools",
  "version": "1.0.0",
  "description": "tool set for neovim",
  "keywords": [
    "tool",
    "decode",
    "encode"
  ],
  "author": "iamcco <ooiss@qq.com>",
  "license": "MIT",
  "main": "./out/index.js",
  "repository": "https://github.com/iamcco/coc-utools",
  "engines": {
    "coc": "^0.0.73"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "vim language server configuration",
      "properties": {
        "utools.debug": {
          "type": "boolean",
          "default": false,
          "description": "enable coc-utools service debug"
        },
        "utools.maxWidth": {
          "type": "number",
          "default": 70,
          "description": "maximum width of utools"
        },
        "utools.trace.server": {
          "type": "string",
          "default": "off",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "description": "Trace level of coc-utools"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "test": "yarn jest"
  },
  "devDependencies": {
    "@chemzqm/neovim": "^5.1.7",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "coc.nvim": "^0.0.73",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "colors": "^1.3.3",
    "decimal.js": "^10.2.0",
    "got": "^9.6.0",
    "tunnel": "^0.0.6",
    "vscode-languageserver-protocol": "^3.14.1"
  }
}
